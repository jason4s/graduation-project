<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#F0F0F4" >

    <RelativeLayout
        android:id="@+id/top"
        android:layout_width="wrap_content"
        android:layout_height="40dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentTop="true"
        android:background="#FF7F00" >

        <Button
            android:id="@+id/left"
            android:layout_width="60dp"
            android:layout_height="40dp"
            android:layout_alignParentLeft="true"
            android:background="@drawable/ic_back"
            android:text=""
            android:textColor="@android:color/white"
            android:textSize="18sp" />
        
        <TextView
            android:layout_width="100dp"
            android:layout_height="40dp"
            android:layout_centerHorizontal="true"
            android:gravity="center"
            android:text="@string/cv"
            android:textColor="@android:color/white"
            android:textSize="20sp" />

        <Button
            android:id="@+id/right"
            android:layout_width="50dp"
            android:layout_height="40dp"
            android:layout_alignParentRight="true"
            android:background="@android:color/transparent"
            android:gravity="center"
            android:text="@string/change"
            android:textColor="@android:color/white"
            android:textSize="18sp" />
    </RelativeLayout>

    <ScrollView
        android:id="@+id/scrollView1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentRight="true"
        android:layout_below="@+id/top" >

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content" >

            <TextView
                android:id="@+id/textView1"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:layout_alignParentLeft="true"
                android:layout_alignParentRight="true"
                android:layout_alignParentTop="true"
                android:layout_marginLeft="10dp"
                android:gravity="center_vertical"
                android:text="@string/person_information"
                android:textColor="#FF7F00"
                android:textSize="20sp" />

            <View
                android:id="@+id/l1"
                android:layout_width="match_parent"
                android:layout_height="2dp"
                android:layout_below="@+id/textView1"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp"
                android:background="#FF7F00" />

            <RelativeLayout
                android:id="@+id/relativeLayout1"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/l1"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp" >

                <TextView
                    android:id="@+id/name"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentTop="true"
                    android:layout_marginTop="10dp"
                    android:text=""
                    android:textSize="20sp" />

                <TextView
                    android:id="@+id/birthday"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignBaseline="@+id/name"
                    android:layout_alignBottom="@+id/name"
                    android:layout_toRightOf="@+id/name"
                    android:text=""
                    android:textSize="16sp" />

                <TextView
                    android:id="@+id/length_of_employment"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_below="@+id/name"
                    android:layout_marginTop="10dp"
                    android:text=""
                    android:textSize="14sp" />

                <TextView
                    android:id="@+id/sex"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/name"
                    android:layout_marginTop="10dp"
                    android:layout_toRightOf="@+id/length_of_employment"
                    android:text=""
                    android:textSize="14sp" />

                <TextView
                    android:id="@+id/marital_status"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/name"
                    android:layout_marginTop="10dp"
                    android:layout_toRightOf="@+id/sex"
                    android:text=""
                    android:textSize="14sp" />

                <TextView
                    android:id="@+id/habitation"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_below="@+id/length_of_employment"
                    android:layout_marginTop="15dp"
                    android:text=""
                    android:textSize="14sp" />

                <TextView
                    android:id="@+id/hukou"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_below="@+id/habitation"
                    android:layout_marginTop="10dp"
                    android:text=""
                    android:textSize="14sp" />
                
                <TextView
                    android:id="@+id/mobile_phone"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_below="@+id/hukou"
                    android:layout_marginTop="10dp"
                    android:text=""
                    android:textSize="14sp" />
                
                <TextView
                    android:id="@+id/mail"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_below="@+id/mobile_phone"
                    android:layout_marginTop="10dp"
                    android:text=""
                    android:textSize="14sp" />
                
                <TextView
                    android:id="@+id/job_status"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_below="@+id/mail"
                    android:layout_marginTop="10dp"
                    android:text=""
                    android:textSize="14sp" />
                
            </RelativeLayout>

            <TextView
                android:id="@+id/textView2"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:layout_alignParentLeft="true"
                android:layout_alignParentRight="true"
                android:layout_below="@+id/relativeLayout1"
                android:layout_marginLeft="10dp"
                android:gravity="center_vertical"
                android:text="@string/job_intention"
                android:textColor="#FF7F00"
                android:textSize="20sp" />

            <View
                android:id="@+id/l2"
                android:layout_width="match_parent"
                android:layout_height="2dp"
                android:layout_below="@+id/textView2"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp"
                android:background="#FF7F00" />

            <RelativeLayout
                android:id="@+id/relativeLayout2"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/l2"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp" >
                
                <TextView
                    android:id="@+id/report_duty"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentTop="true"
                    android:layout_marginTop="10dp"
                    android:text=""
                    android:textSize="14sp" />
                
                <TextView
                    android:id="@+id/job_type"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_below="@+id/report_duty"
                    android:layout_marginTop="10dp"
                    android:text=""
                    android:textSize="14sp" />
                
                <TextView
                    android:id="@+id/want_industry"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_below="@+id/job_type"
                    android:layout_marginTop="10dp"
                    android:text=""
                    android:textSize="14sp" />
                
                <TextView
                    android:id="@+id/location"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_below="@+id/want_industry"
                    android:layout_marginTop="10dp"
                    android:text=""
                    android:textSize="14sp" />
                
                <TextView
                    android:id="@+id/want_salary"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_below="@+id/location"
                    android:layout_marginTop="10dp"
                    android:text=""
                    android:textSize="14sp" />
                
                <TextView
                    android:id="@+id/Aim_Function"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_below="@+id/want_salary"
                    android:layout_marginTop="10dp"
                    android:text=""
                    android:textSize="14sp" />
                
            </RelativeLayout>

            <TextView
                android:id="@+id/textView3"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:layout_alignParentLeft="true"
                android:layout_alignParentRight="true"
                android:layout_below="@+id/relativeLayout2"
                android:layout_marginLeft="10dp"
                android:gravity="center_vertical"
                android:text="@string/education"
                android:textColor="#FF7F00"
                android:textSize="20sp" />

            <View
                android:id="@+id/l3"
                android:layout_width="match_parent"
                android:layout_height="2dp"
                android:layout_below="@+id/textView3"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp"
                android:background="#FF7F00" />

            <RelativeLayout
                android:id="@+id/relativeLayout3"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/l3"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp" >
                
                <TextView
                    android:id="@+id/school"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentTop="true"
                    android:layout_marginTop="10dp"
                    android:text=""
                    android:textSize="14sp" />
                
                <TextView
                    android:id="@+id/level_of_education"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_below="@+id/school"
                    android:layout_marginTop="10dp"
                    android:text=""
                    android:textSize="14sp" />
                
            </RelativeLayout>

            <TextView
                android:id="@+id/textView4"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:layout_alignParentLeft="true"
                android:layout_alignParentRight="true"
                android:layout_below="@+id/relativeLayout3"
                android:layout_marginLeft="10dp"
                android:gravity="center_vertical"
                android:text="@string/skill"
                android:textColor="#FF7F00"
                android:textSize="20sp" />

            <View
                android:id="@+id/l4"
                android:layout_width="match_parent"
                android:layout_height="2dp"
                android:layout_below="@+id/textView4"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp"
                android:background="#FF7F00" />

            <RelativeLayout
                android:id="@+id/relativeLayout4"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/l4"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp" >
                
                <TextView
                    android:id="@+id/skills"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentTop="true"
                    android:layout_marginTop="10dp"
                    android:text=""
                    android:textSize="14sp" />
                
                <View 
                    android:layout_width="match_parent"
                    android:layout_height="20dp"
                    android:layout_below="@+id/skills"/>
                
            </RelativeLayout>
        </RelativeLayout>
    </ScrollView>

</RelativeLayout>